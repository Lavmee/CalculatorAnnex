<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CalculatorAnnex" parent="Theme.Material3.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/white</item>
        <item name="colorSecondaryVariant">@color/white</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Navigation Bar color. -->
        <item name="android:navigationBarColor">@color/darkWhite</item>
        <!-- Background color. -->
        <item name="backgroundColor">@color/white</item>
        <!-- Light Status Bar. -->
        <item name="android:windowLightStatusBar">true</item>
        <!-- Background Keyboard color. -->
        <item name="backgroundKeyboardColor">@color/darkWhite</item>
        <!-- Background Key color. -->
        <item name="backgroundKeyColor">@color/white</item>
        <!-- Key color. -->
        <item name="keyColorPrimary">@color/black</item>
        <item name="keyColorSecondary">@color/cyanDark</item>
        <item name="keyColorTertiary">@color/peach</item>
    </style>
</resources>